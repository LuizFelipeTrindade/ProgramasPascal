Program somaandleitura;

var a, b : integer;

var c : String ;

begin

c := 'Hi,Welcome, esse programa subtrai, multiplica e soma valores';
writeln(c);

write('Digite um valor: ');
read (a);

write('Digite novamente: ');
read (b);

write('Soma: ');
writeln(a+b);

write('Multi: ');
writeln(a*b);

write('Sub: ');
writeln(a - b);

end.